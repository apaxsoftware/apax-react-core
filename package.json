{
  "name": "apax-react-core",
  "version": "1.2.0",
  "description": "Contains core react functionality developed by Apax Software LLC",
  "author": "apaxsoftware",
  "license": "MIT",
  "repository": "https://github.com/apaxsoftware/apax-react-core",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "test": "react-scripts test",
    "test:watch": "react-scripts test --env=jsdom",
    "test:coverage": "yarn test --coverage",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "lint": "eslint ./src --ext .js --ignore-pattern node_modules"
  },
  "peerDependencies": {
    "axios": "^0.19.2",
    "lodash": "^4.17.15",
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "universal-cookie": "^4.0.3"
  },
  "devDependencies": {
    "@redux-saga/is": "^1.1.2",
    "@redux-saga/symbols": "^1.1.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "@svgr/rollup": "^2.4.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "gh-pages": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "lodash": "^4.17.15",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "^1.1.4",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "redux-saga-test-plan": "^4.0.0-rc.3",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-url": "^1.4.0",
    "universal-cookie": "^4.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/sagas/**/*.js",
      "!src/**/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    }
  },
  "resolutions": {
    "handlebars": "4.5.0"
  },
  "files": [
    "dist"
  ]
}
